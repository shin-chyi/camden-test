{
  "components/common/header": "{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n<header class=\"header header-logo--{{theme_settings.logo-position}}\" role=\"banner\">\r\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n    </a>\r\n\r\n  <div class=\"logo-user-section container\">\r\n    {{#if page_type '===' 'default'}}\r\n        <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n            {{> components/common/store-logo}}\r\n        </h1>\r\n        {{> components/common/navigation}}\r\n    {{else}}\r\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n            {{> components/common/store-logo}}\r\n        </div>\r\n        {{> components/common/navigation}}\r\n    {{/if}}\r\n  </div>\r\n    <div id=\"menu-wrapper\">\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n          {{> components/common/navigation-menu}}\r\n          <div class=\"clear\"></div>\r\n        </div>\r\n      <div class=\"clear\"></div>\r\n    </div>\r\n    <div class=\"clear\"></div>\r\n\r\n</header>\r\n<div class=\"below_nav_container\">\r\n    {{{region name=\"header_below_nav--global\"}}}\r\n</div>\r\n<div class=\"below_nav_clear clear\" style=\"margin-top: 0px;\"></div>\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <div class=\"header-logo-image-container\">\r\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            </div>\r\n        {{/if}}\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n",
  "components/common/navigation": "<nav class=\"navUser\">\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n        {{#if customer.store_credit.value '>' 0}}\r\n            <li class=\"navUser-item navUser-item--storecredit desktop\">\r\n                <a class=\"navUser-action navUser-action--storeCredit\"\r\n                   data-dropdown=\"storeCredit-dropdown\"\r\n                   data-options=\"align:bottom\"\r\n                   href=\"{{urls.cart}}\">\r\n                        <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\r\n                </a>\r\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\r\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\r\n                </div>\r\n                <div class=\"clear\"></div>\r\n            </li>\r\n        {{/if}}\r\n\r\n        <div class=\"dropdown dropdown--quickSearch desktop\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n            {{> components/common/quick-search}}\r\n        </div>\r\n\r\n        <div class=\"user-icons\">\r\n          <!-- <li class=\"navUser-item navUser-item--cart user-icons\">\r\n              <a\r\n                  class=\"navUser-action\"\r\n                  data-cart-preview\r\n                  data-dropdown=\"cart-preview-dropdown\"\r\n                  data-options=\"align:right\"\r\n                  href=\"{{urls.cart}}\"\r\n                  aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n                  <span class=\"navUser-item-cartLabel\"><svg class=\"header-icon cart-icon\"><use xlink:href=\"#icon-cart\"></use></svg></span> <span class=\"countPill cart-quantity\"></span>\r\n              </a>\r\n\r\n              <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n          </li> -->\r\n\r\n          <li class=\"navUser-item navUser-item--cart user-icons\">\r\n            <a class=\"navUser-action\"\r\n               data-cart-preview\r\n               data-dropdown=\"cart-preview-dropdown\"\r\n               data-options=\"align:right\"\r\n               href=\"{{urls.cart}}\"\r\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n            >\r\n                <!-- <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\r\n                <span class=\"countPill cart-quantity\"></span> -->\r\n                <span class=\"navUser-item-cartLabel\"><svg class=\"header-icon cart-icon\"><use xlink:href=\"#icon-cart\"></use></svg></span> <span class=\"countPill cart-quantity\"></span>\r\n            </a>\r\n\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n\r\n          {{>components/common/currency-selector}}\r\n\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navUser-item user-icons gift-certificates\">\r\n                <a alt=\"Gift Certificates\" class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\"><svg class=\"header-icon gift-icon\"><use xlink:href=\"#icon-gift\" /></svg></a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav><svg class=\"header-icon compare-icon\"><use xlink:href=\"#icon-compare\"></use></svg><span class=\"countPill countPill--positive countPill--alt compare\"></span></a>\r\n\r\n        <!-- <li class=\"navUser-item navUser-item--account user-icons account\"> -->\r\n                {{#if settings.account_creation_enabled}}\r\n                    <!-- <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"><svg class=\"header-icon account-icon\"><use xlink:href=\"#icon-account\"></use></svg></a> -->\r\n                    {{> components/common/account-link}}\r\n                {{/if}}\r\n        <!-- </li> -->\r\n\r\n        <li class=\"navUser-item quicksearch mobile\">\r\n            <a class=\"navUser-action navUser-action--quickSearch desktop-toggle\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\"><svg class=\"header-icon search-icon\"><use xlink:href=\"#icon-search\"></use></svg></a>\r\n        </li>\r\n\r\n    </ul>\r\n    </div>\r\n\r\n</nav>\r\n",
  "components/common/navigation-menu": "<div class=\"navPages-wider desktop\">\r\n  {{#if theme_settings.show_logo_in_sticky_nav '===' true}}\r\n  <li class=\"navPages-item sticky-object logo\">\r\n    {{> components/common/sticky-logo}}\r\n  </li>\r\n  {{/if}}\r\n  <div class=\"nav-minus-logo\">\r\n  <div class=\"mega-cat-nav {{#if theme_settings.show_logo_in_sticky_nav '===' true}}sticky-logo{{/if}}\">\r\n    <li class=\"navPages-item navPages-item-page container-header btn\">\r\n      <a class=\"navPages-action\">\r\n        <svg class=\"mega-menu-icon open\"><use xlink:href=\"#icon-v-menu\" /></svg><svg class=\"mega-menu-icon close\"><use xlink:href=\"#icon-close\" /><span class=\"burger-menu\">{{theme_settings.mega_nav_name}}</span><span class=\"burger-menu fixed\">{{theme_settings.sticky_mega_nav_name}}</span>\r\n      </a>\r\n      <div class=\"clear\"></div>\r\n    </li>\r\n    <nav class=\"navPages\">\r\n      <div class=\"mega-cat-dropdown\">\r\n        <div class=\"inner\">\r\n\r\n        <div id=\"page-wrapper\">\r\n        <div class=\"category-list\">\r\n          <h2 class=\"container-header mobile\">Categories</h2>\r\n        {{#each categories}}\r\n          <li class=\"navPages-item top-level categories{{#if is_active}} activePage{{/if}}\">\r\n          {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n              {{> components/common/navigation-list}}\r\n          {{/if}}\r\n          {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n              {{> components/common/navigation-list-alternate}}\r\n          {{/if}}\r\n          </li>\r\n        {{/each}}\r\n        </div>\r\n      <div class=\"pagination-container\"><a class=\"navPages-action previous\"><i class=\"icon prev\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-left\" /></svg></i></a><span class=\"dots\"></span><a class=\"navPages-action next\"><i class=\"icon next\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-left\" /></svg></i></a></div>\r\n      <hr class=\"sidenav\"/>\r\n        </div>\r\n        </div>\r\n\r\n        {{#if theme_settings.show_pages_in_mobile_nav '===' true}}\r\n        <div class=\"sticky-navpages\">\r\n          <h2 class=\"container-header mobile\">Navigation</h2>\r\n          {{#each pages}}\r\n            <li class=\"navPages-item top-level{{#if is_active}} activePage{{/if}} pages mega-nav-pages\">\r\n                {{> components/common/mega-nav-page-list}}\r\n            </li>\r\n          {{/each}}\r\n        </div>\r\n        {{/if}}\r\n\r\n        <!-- BRANDS VISIBILITY -->\r\n        <div class=\"brands {{#if theme_settings.show_brands_in_main_mega_nav '===' false}}brands-hidden {{/if}}{{#if theme_settings.show_brands_in_sticky_nav '===' false}}sticky-brands-hidden{{/if}}\">\r\n          <h2 class=\"container-header mobile\">Brands</h2>\r\n            {{#each shop_by_brand}}\r\n                   <li class=\"navPages-item mobile\">\r\n                        <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n            {{/each}}\r\n                    <li class=\"navPages-item mobile view-all brands\">\r\n                        <a class=\"navPages-action\" href=\"/brands/\">View all Brands</a>\r\n                    </li>\r\n        </div>\r\n        {{#if customer.store_credit.value '>' 0}}\r\n        <ul class=\"nav-user-menu\">\r\n            <li class=\"navUser-item navUser-item--storecredit\">\r\n                <a class=\"navUser-action navUser-action--storeCredit\"\r\n                   data-dropdown=\"storeCredit-dropdown\"\r\n                   data-options=\"align:bottom\"\r\n                   href=\"{{urls.cart}}\">\r\n                        <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\r\n                </a>\r\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\r\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\r\n                </div>\r\n                <div class=\"clear\"></div>\r\n            </li>\r\n        </ul>\r\n        {{/if}}\r\n            <div class=\"clear\"></div>\r\n\r\n        <div class=\"clear\"></div>\r\n      </div>\r\n      <div class=\"clear\"></div>\r\n    </div>\r\n    <div class=\"nav-right-fixed\">\r\n    <div class=\"nav-right\">\r\n    <ul class=\"navPages-list nav-page-list one {{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n      {{#unless theme_settings.hide_category_navigation}}\r\n        {{#each categories}}\r\n            <li class=\"navPages-item categories\">\r\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                {{> components/common/navigation-list}}\r\n            {{/if}}\r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n                {{> components/common/navigation-list-alternate}}\r\n            {{/if}}\r\n            </li>\r\n        {{/each}}\r\n        {{/unless}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n        <ul class=\"navPages-list nav-page-list two\">\r\n       {{#each pages}}\r\n           <li class=\"navPages-item\">\r\n               {{> components/common/navigation-page-list}}\r\n           </li>\r\n       {{/each}}\r\n       {{#if settings.phone_number}}\r\n         <li class=\"navPages-item navPages-item-page pages phone-number\">\r\n           <a class=\"navPages-action phone-number\" href=\"tel:{{settings.phone_number}}\">{{settings.phone_number}}</a>\r\n         </li>\r\n       {{/if}}\r\n   </ul>\r\n        {{/unless}}\r\n\r\n    </ul>\r\n    </div>\r\n\r\n\r\n\r\n</nav>\r\n\r\n</div>\r\n\r\n</div>\r\n</div>\r\n",
  "components/common/account-link": "{{#if settings.account_creation_enabled}}\r\n<!-- <ul class=\"account-link enabled\"> -->\r\n    <li class=\"navUser-item navUser-item--account user-icons account\">\r\n      {{#if customer}}\r\n      <!-- <a class=\"navUser-action with-logout\" href=\"{{urls.account.index}}\"><svg><use xlink:href=\"#icon-profile\" /></svg></a> -->\r\n      <!-- <a class=\"navUser-action logout\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a> -->\r\n      <a class=\"navUser-action account signout-list\" alt=\"User Toolbox\" href=\"javascript:void(0)\"><svg class=\"header-icon account-icon\"><use xlink:href=\"#icon-account\"></use></svg></a>\r\n      <div class=\"dropdown-menu signout-list\" id=\"account-dropdown-signout-list\">\r\n        <!-- <h2 class=\"container-header\">Account Info</h2> -->\r\n          {{> components/common/account-dropdown-signout}}\r\n        <a class=\"navBar-action signout-list\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n        <!-- <hr class=\"left full-width\" /> -->\r\n        <!-- { {> components / common / currency-selector-dropdown } } -->\r\n      </div>\r\n      {{else}}\r\n        <a class=\"navUser-action account signin-list\" alt=\"User Toolbox\" href=\"{{urls.auth.login}}\"><svg class=\"header-icon account-icon\"><use xlink:href=\"#icon-account\"></use></svg></a>\r\n        <!-- <a class=\"navUser-action login\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> -->\r\n        <div class=\"dropdown-menu signin-list\" id=\"account-dropdown-signin-list\">\r\n          <!-- <h2 class=\"container-header\">Account Info</h2> -->\r\n            {{> components/common/account-dropdown-signin}}\r\n            <!-- <hr class=\"left full-width\" /> -->\r\n            <!-- { {> components / common / currency-selector-dropdown } } -->\r\n            <!-- <a class=\"button button--primary signin-register logout-button\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> -->\r\n        </div>\r\n      {{/if}}\r\n     </li>\r\n{{/if}}\r\n<!-- </ul> -->\r\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\r\n<li class=\"navUser-item currency-menu\">\r\n    <a class=\"navUser-action has-dropdown\" alt=\"Currency Selector\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{currency_selector.active_currency_code}} <i class=\"icon currency\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\r\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n        {{#each currency_selector.currencies}}\r\n        <li class=\"dropdown-menu-item\">\r\n              <a class=\"currency-name-switcher\" href=\"{{{switch_url}}}\"\r\n                   data-currency-code=\"{{{code}}}\"\r\n                   {{#if cart_currency_switch_url.length '>' 0 }}\r\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\r\n                   {{/if}}\r\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\r\n                >\r\n                {{#if is_active}}\r\n                <strong>{{{flag}}} {{name}}</strong>\r\n                {{else}}\r\n                {{{flag}}} {{name}}\r\n                {{/if}}\r\n            </a>\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n</li>\r\n{{/if}}\r\n",
  "components/common/quick-search": "<div class=\"container\">\r\n  <a href=\"#\" class=\"quickSearch-mobileToggle\" alt=\"Submit Search\">\r\n      <span class=\"mobileMenu-toggleIcon\"></span>\r\n  </a>\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                <!-- <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\"><svg class=\"header-icon search-icon\"><use xlink:href=\"#icon-search\"></use></svg> -->\r\n            </div>\r\n        </fieldset>\r\n        <div class=\"search-icon submit\">\r\n          <input alt=\"Submit Search\" class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\r\n          <svg class=\"header-icon search-icon\" title=\"submit\" alt=\"submit\"><use xlink:href=\"#icon-search\"></use></svg>\r\n        </div>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n</div>\r\n",
  "components/common/sticky-logo": "<a class=\"navPages-action\" href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <div class=\"header-logo-image-container\">\r\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            </div>\r\n        {{/if}}\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n",
  "components/common/navigation-list": "{{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root second-level-parent sideNavTopLevel {{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n        {{name}} <i class=\"icon navPages-action-moreIcon topLevel\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n    </a>\r\n    {{> components/common/navigation-simple-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action single-level sideNavTopLevel {{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n",
  "components/common/mega-nav-page-list": "<div class=\"nav-page-list mega-nav-page-list\">\r\n    {{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root second-level-parent topLevel sideNavTopLevel\" href=\"{{url}}\">\r\n        {{name}} <i class=\"icon mega-nav-pages\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n        <i class=\"icon navPages-action-moreIcon fixed-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n    </a>\r\n    <div class=\"navPage-subMenu navPage-subMenu-horizontal second-level-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <ul class=\"navPage-subMenu-list\">\r\n    \r\n            {{#each children}}\r\n                <li class=\"navPage-subMenu-item-child second-level\">\r\n                    {{#if children}}\r\n                        <a\r\n                            class=\"navPage-subMenu-action navPages-action has-subMenu second-level\"\r\n                            href=\"{{url}}\">\r\n                            {{name}} <i class=\"icon mega-nav-pages\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                        </a>\r\n                        <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                            {{#each children}}\r\n                            <li class=\"navPage-childList-item\">\r\n                                <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}} </a>\r\n                            </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                    {{else}}\r\n                        <a class=\"navPage-subMenu-action navPages-action second-level\" href=\"{{url}}\">{{name}}</a>\r\n                    {{/if}}\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n    {{else}}\r\n    <a class=\"navPages-action sideNavTopLevel\" href=\"{{url}}\">{{name}}</a>\r\n    {{/if}}\r\n    <div class=\"clear\"></div>\r\n    </div>\r\n    \r\n",
  "components/common/navigation-page-list": "<div class=\"nav-page-list\">\r\n{{#if children}}\r\n<a class=\"navPages-action navPages-action-depth-max has-subMenu is-root topLevel\" href=\"{{url}}\">\r\n    {{name}} <i class=\"fa fa-chevron-down main-nav desktop-only\" aria-hidden=\"true\"></i>\r\n    <i class=\"icon navPages-action-moreIcon fixed-dropdown mobile-only\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n</a>\r\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a\r\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\r\n                        href=\"{{url}}\"\r\n                        data-collapsible=\"navPages-{{id}}\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\"\r\n                        data-collapsible-disabled-state=\"open\"\r\n                        data-collapsible-enabled-state=\"closed\">\r\n                        {{name}}\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        {{#each children}}\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n{{else}}\r\n<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n<div class=\"clear\"></div>\r\n</div>\r\n",
  "components/common/navigation-list-alternate": "<div class=\"desktop\">\r\n{{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root second-level-parent sideNavTopLevel needsclick{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}\r\n    <i class=\"icon navPages-action-moreIcon closed topLevel\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n    </a>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action single-level sideNavTopLevel needsclick{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n</div>\r\n\r\n<div class=\"mobile\">\r\n{{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root second-level-parent sideNavTopLevel needsclick{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}\r\n    </a>\r\n     <i class=\"icon navPages-action-moreIcon closed topLevel\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action single-level sideNavTopLevel needsclick{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n</div>\r\n",
  "components/common/account-dropdown-signout": "<nav class=\"navBar navBar--sub navBar--account signout-list\">\r\n    <ul class=\"navBar-section signout-list\">\r\n        {{#if account_page '===' 'orders'}}\r\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.orders'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item signout-list\">\r\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.returns_enabled}}\r\n            {{#if account_page '===' 'returns'}}\r\n                <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.returns'}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item signout-list\">\r\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if account_page '===' 'messages'}}\r\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item signout-list\">\r\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.show_payment_methods}} \r\n            {{#if account_page '===' 'payment_methods'}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.payment_methods'}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item signout-list\">\r\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}} \r\n\r\n        {{#if account_page '===' 'addresses'}}\r\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.addresses'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item signout-list\">\r\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if settings.show_wishlist}}\r\n            {{#if account_page '===' 'wishlists'}}\r\n                <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item signout-list\">\r\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if account_page '===' 'recent_items'}}\r\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.recently_viewed'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item signout-list\">\r\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if account_page '===' 'settings'}}\r\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.settings'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item signout-list\">\r\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n",
  "components/common/account-dropdown-signin": "<nav class=\"navBar navBar--sub navBar--account signin-list\">\r\n    <ul class=\"navBar-section signout-list\">\r\n      <li>\r\n        <a class=\"navBar-action signout-list\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}  </a>\r\n          <span class=\"navUser-or\"> {{ lang 'common.or' }} </span>\r\n          <a class=\"navBar-action signout-list\" href=\"{{urls.auth.create_account}}\">  {{lang 'common.sign_up'}}</a></li>\r\n    </ul>\r\n</nav>\r\n",
