n-third-level}}\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">{{name}}</a>\r\n                    <!-- <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max view-all\" href=\"{{url}}\">View All</a> -->\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n        <!-- <div class=\"flyout-pagination-container bottom\">\r\n          <a class=\"navPages-action previous\">\r\n            <i class=\"icon prev\" aria-hidden=\"true\">\r\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\r\n            </i>\r\n          </a>\r\n          <span class=\"dots\"></span>\r\n          <a class=\"navPages-action next\">\r\n            <i class=\"icon next\" aria-hidden=\"true\">\r\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\r\n            </i>\r\n          </a>\r\n        </div> -->\r\n\r\n    </ul>\r\n    </div>\r\n\r\n<div class=\"mobile\">\r\n    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\">\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item-child second-level\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">\r\n                        {{name}}\r\n                    </a>\r\n                    <i class=\"icon navPages-action-moreIcon second-level\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    {{> components/common/navigation-dropdown-third-level}}\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">{{name}}</a>\r\n                    <!-- <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max view-all\" href=\"{{url}}\">View All</a> -->\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n        <!-- <div class=\"flyout-pagination-container bottom\">\r\n          <a class=\"navPages-action previous\">\r\n            <i class=\"icon prev\" aria-hidden=\"true\">\r\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\r\n            </i>\r\n          </a>\r\n          <span class=\"dots\"></span>\r\n          <a class=\"navPages-action next\">\r\n            <i class=\"icon next\" aria-hidden=\"true\">\r\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\r\n            </i>\r\n          </a>\r\n        </div> -->\r\n\r\n    </ul>\r\n    </div>\r\n</div>\r\n",
