{
  "components/common/breadcrumbs": "<ul class=\"breadcrumbs\">\r\n    {{#each breadcrumbs}}\r\n        <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\">\r\n            {{#or @last (if url \"==\" null)}}\r\n                \r\n                <span class=\"breadcrumb-label\">{{name}}</span>\r\n            {{else}}\r\n                <a href=\"{{url}}\" class=\"breadcrumb-label\"><span>{{name}}</span></a>\r\n            {{/or}}\r\n            \r\n        </li>\r\n    {{/each}}\r\n</ul>\r\n"
}
