{
  "components/common/forms/selectortext": "<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <noscript>\r\n        <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n            {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\r\n        </label>\r\n        <input type=\"text\"\r\n               id=\"{{id}}_input\"\r\n               name=\"{{name}}\"\r\n               value=\"{{value}}\"\r\n               class=\"form-input\"\r\n               aria-required=\"{{required}}\"\r\n               data-label=\"{{label}}\"\r\n               data-input\r\n        />\r\n    </noscript>\r\n\r\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select name=\"{{name}}\"\r\n            class=\"form-select\"\r\n            aria-required=\"{{required}}\"\r\n            id=\"{{id}}_select\"\r\n            data-label=\"{{label}}\"\r\n            data-input\r\n            {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\r\n    >\r\n        {{#if options}}\r\n            <option value=\"\">{{chooseprefix}}</option>\r\n            {{#each options}}\r\n                <option {{#if selected}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n            {{/each}}\r\n        {{/if}}\r\n    </select>\r\n</div>\r\n"
}
